  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6e6e6e"
      }
    },
    {
      "name": "Storage types. classes ,const ,let etc",
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#FBBED4"
      }
    },
    {
      "name": " Storage modifier. implements",
      "scope": [
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#FBBED4"
      }
    },
    {
      "name": " keyword. if, this, new ,etc",
      "scope": [
        "keyword",
        "variable.language",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#FBBED4"
      }
    },
    {
      "name": " keyword operator. =,etc",
      "scope": [
