  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#868686"
      }
    },
    {
      "name": "Storage types. classes ,const ,let etc",
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#cf2c70"
      }
    },
    {
      "name": " Storage modifier. implements",
      "scope": [
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#cf2c70"
      }
    },
    {
      "name": " keyword. if, this, new ,etc",
      "scope": [
        "keyword",
        "variable.language",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#cf2c70"
      }
    },
    {
      "name": " keyword operator. =,etc",
      "scope": [
        "keyword.operator"
